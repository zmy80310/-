/* pages/find/prize/prize.wxss */
page{
   background: rgb(245, 242, 242)
}
.prize{
  margin: 10px 0;
  box-shadow: 0 1px 5px 0px #999;

}
 .prize-img{
   width: 100%;
   height: 200px;
 }
 .prize-img>image{
   display: block;
   width: 100%;
   height: 100%;
 }
 .prize-detail{
   padding: 10px 10px;
   background-color: #fff;
 }
 .prize-tit,pri-des{
   margin: 5px 0;
 }
 .pri-des{
   font-size: 13px;
   color: #999;
 }